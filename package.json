{
  "name": "terriblecards",
  "version": "0.0.0",
  "author": "Logan Saunders <gigavinyl@gmail.com>",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "devstart": "webpack && node app.js"
  },
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.2.0",
    "jade": "~1.11.0",
    "koa": "^1.1.2",
    "koa-compress": "^1.0.8",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.3.0",
    "koa-socket": "^4.0.0",
    "koa-static": "^1.5.2",
    "koa-views": "^3.1.0",
    "socket.io": "^1.4.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "no-console": 0,
      "dot-notation": 0,
      "padded-blocks": 0,
      "no-param-reassign": 0,
      "no-multiple-empty-lines": 0,
      "func-names": 0
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "globals": {
      "socket": true
    },
    "ecmaFeatures": {
      "modules": true,
      "generators": true
    },
    "ecmaVersion": 6,
    "extends": "airbnb",
    "plugins": [
      "html"
    ]
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.3.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "bootstrap": "^3.3.6",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "jade": "^1.11.0",
    "jquery": "^2.2.0",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "vue": "^1.0.16",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.1.0",
    "vue-loader": "^8.1.1",
    "vue-router": "^0.7.11",
    "vue-style-loader": "^1.0.0",
    "vuex": "^0.2.0",
    "webpack": "^1.12.13"
  }
}
